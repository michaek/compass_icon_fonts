$<%= variable %>-files: "<%= files.join('", "') %>";

$<%= variable %>-characters:<% last = pairs.values.last %>
<% pairs.each do |classname, character| %>
    "<%= classname %>", "<%= character %>"<%= character == last ? ';' : ',' %><% end %>
